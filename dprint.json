{
	"typescript": {
		"useTabs": true,
		"quoteProps": "asNeeded"
	},
	"json": {
		"useTabs": true
	},
	"markdown": {
		"lineWidth": 100
	},
	"malva": {
		"printWidth": 100,
		"useTabs": true
	},
	"excludes": [
		"web/node_modules",
		"web/dist",
		"web/elm-stuff",
		"web/.parcel-cache"
	],
	"exec": {
		"commands": [
			{
				"command": "npx elm-format --stdin",
				"exts": ["elm"]
			},
			{
				"command": "gofmt",
				"exts": ["go"]
			},
			{
				"command": "swiftformat stdin --config apple_universal/.swiftformat",
				"exts": ["swift"]
			}
		]
	},
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.91.1.wasm",
		"https://plugins.dprint.dev/json-0.19.3.wasm",
		"https://plugins.dprint.dev/markdown-0.17.1.wasm",
		"https://plugins.dprint.dev/exec-0.4.4.json@c207bf9b9a4ee1f0ecb75c594f774924baf62e8e53a2ce9d873816a408cecbf7",
		"https://plugins.dprint.dev/g-plane/malva-v0.4.0.wasm"
	]
}
